<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.facewindu.breastfeedingbuddy"
    android:versionCode="7"
    android:versionName="1.0.6" >

    <supports-screens android:xlargeScreens="true" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:debuggable="false"
        android:icon="@mipmap/ic_launcher"
        android:label="BreastFeedingBuddyFX">
        <activity
            android:name="javafxports.android.FXActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation|screenSize"
            android:label="BreastFeedingBuddyFX" >
            <meta-data
                android:name="main.class"
                android:value="org.facewindu.breastfeedingbuddy.BreastFeedingBuddyFX" />
            <meta-data
                android:name="debug.port"
                android:value="0" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>